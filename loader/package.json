{
  "name": "sk-all-in-one-loader",
  "version": "1.0.4",
  "description": "Premium All-In-One Cheat Loader",
  "main": "main.js",
  "author": "SK-Development",
  "scripts": {
    "start": "electron .",
    "start-panel": "electron panel-main.js",
    "build": "electron-builder",
    "build-panel": "electron-builder --config electron-builder-panel.json",
    "publish": "electron-builder --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffyNoJumper/My_Auth_API"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.4",
    "electron-updater": "^6.1.7",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.2.1",
    "node-addon-api": "^7.1.0"
  },
  "devDependencies": {
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.sk.allinone",
    "productName": "SK ALL-IN-ONE",
    "extraResources": [
      {
        "from": "assets",
        "to": "assets",
        "filter": [ "**/*" ]
      }
    ],
    "copyright": "Copyright Â© 2026 SK-Development",
    "directories": {
      "output": "SK ALL IN-ONE LOADER"
    },
    "files": [
      "**/*",
      "build/Release/spoofer.node",
      "imgs/*",
      "assets/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "jeffyNoJumper",
        "repo": "My_Auth_API"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "SK.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true
    }
  }
}
